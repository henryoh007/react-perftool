{
	"name": "react-perftool",
	"version": "1.0.0",
	"main": "./build/lib/react-perftool.js",
	"description":
		"A browser developer tools extension, which will help you to inspect the performance of React Js components.",
	"repository": "https://github.com/vaheqelyan/react-perftool.git",
	"author": "Vahe <martinkenner@mail.com>",
	"license": "MIT",
	"keywords": ["react", "performance", "chrome", "developer tools", "chrome extension"],
	"scripts": {
		"test-server": "parcel ./test/index.html -p 80",
		"devtool-dev": "parcel watch ./src/chrome/panel/panel.html --out-dir ./extension-build",
		"lib-dev": "rm -rf ./build && rollup -c -w",
		"lib-build": "rm -rf ./build && rollup -c",
		"devtool-build": "parcel build ./src/chrome/panel/panel.html --out-dir ./extension-buil"
	},
	"devDependencies": {
		"@types/chrome": "^0.0.58",
		"@types/react": "^16.0.34",
		"@types/react-dom": "^16.0.3",
		"@types/react-redux": "^5.0.14",
		"@types/react-virtualized": "^9.7.10",
		"node-sass": "^4.7.2",
		"rollup": "^0.51.3",
		"rollup-plugin-alias": "^1.4.0",
		"rollup-plugin-commonjs": "^8.2.6",
		"rollup-plugin-node-globals": "^1.1.0",
		"rollup-plugin-node-resolve": "^3.0.0",
		"rollup-plugin-replace": "^2.0.0",
		"rollup-plugin-typescript2": "^0.10.0",
		"rollup-watch": "^4.3.1",
		"typescript": "^2.6.2"
	},
	"dependencies": {
		"lodash.omit": "^4.5.0",
		"lodash.sortby": "^4.7.0",
		"mobx": "^3.4.1",
		"mobx-react": "^4.3.5",
		"react": "^16.2.0",
		"react-awesome-popover": "^1.6.3",
		"react-dom": "^16.2.0",
		"react-virtualized": "^9.18.0"
	}
}
